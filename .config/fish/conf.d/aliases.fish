alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

alias ls='ls --group-directories-first --color=auto'
alias la='ls -A --group-directories-first'
alias ll='ls -lA --group-directories-first'

alias grep='grep --color=auto'

alias diff='diff --color=auto'

alias ip='ip -color=auto'
